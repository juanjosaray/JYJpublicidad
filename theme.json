			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#000000",
						"#FFFFFF"
					],
					"name": "Duotone 1",
					"slug": "duotone-1"
				},
				{
					"colors": [
						"#000000",
						"#BBBBBB"
					],
					"name": "Duotone 2",
					"slug": "duotone-2"
